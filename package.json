{
  "name": "mocha-webpack",
  "version": "0.0.1-alpha",
  "description": "mocha cli with webpack support",
  "bin": {
    "mocha-webpack": "./bin/_mocha"
  },
  "files": [
    "*.md",
    "bin",
    "src",
    "lib",
    "modules/*.js",
    "npm-scripts"
  ],
  "scripts": {
    "build": "rimraf lib && babel ./src -d lib --ignore '__test__'",
    "lint": "eslint src"
  },
  "keywords": [
    "wepback",
    "mocha"
  ],
  "author": "Jan-Andr√© Zinser",
  "license": "MIT",
  "peerDependencies": {
    "mocha": "^2.4.5",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "invariant": "^2.2.0",
    "lodash": "^4.3.0",
    "webpack-sources": "^0.1.1"
  }
}
