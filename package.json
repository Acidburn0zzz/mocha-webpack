{
  "name": "mocha-webpack",
  "version": "0.0.1-alpha.2",
  "description": "mocha cli with webpack support",
  "bin": {
    "mocha-webpack": "./bin/_mocha"
  },
  "files": [
    "*.md",
    "bin",
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "node scripts/clean.js",
    "build": "npm run clean && babel ./src -d lib --ignore '__test__'",
    "lint": "eslint src bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zinserjan/mocha-webpack"
  },
  "bugs": {
    "url": "https://github.com/zinserjan/mocha-webpack/issues"
  },
  "keywords": [
    "webpack",
    "mocha"
  ],
  "author": "Jan-Andr√© Zinser",
  "license": "MIT",
  "peerDependencies": {
    "mocha": "^2.4.5",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1"
  },
  "dependencies": {
    "anymatch": "^1.3.0",
    "fs-extra": "^0.26.5",
    "glob-parent": "^2.0.0",
    "invariant": "^2.2.0",
    "is-glob": "^2.0.1",
    "lodash": "^4.3.0",
    "object-hash": "^1.1.2",
    "webpack-sources": "^0.1.1",
    "yargs": "^3.32.0"
  }
}
